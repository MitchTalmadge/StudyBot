{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "src/*": [
          "src/*"
        ],
        "*": [
          "node_modules/*"
        ],
      }
    },
    "angularCompilerOptions": {
      "enableIvy": true
    },
    "include": [
      "src/**/*"
    ]
  }
  