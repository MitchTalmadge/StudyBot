{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": 1,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "eol-last": "warn",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "import/no-unresolved": 0,
    "import/order": "warn",
    "import/prefer-default-export": 0,
    "indent": "warn",
    "max-len": "warn",
    "no-console": 0,
    "no-trailing-spaces": "warn",
    "object-curly-spacing": "warn",
    "padded-blocks": "warn",
    "quotes": "warn",
    "semi": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}